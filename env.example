# Update these with your Supabase details from your project settings > API
NEXT_PUBLIC_SUPABASE_URL=your-project-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Resend API Key for Emails
RESEND_API_KEY=re_123456789
RESEND_FROM_EMAIL=InnovaPort <noreply@innovaport.com>

# Square Keys
SQUARE_ACCESS_TOKEN=sq0atp-...
SQUARE_APPLICATION_ID=sq0idp-...
SQUARE_LOCATION_ID=...
SQUARE_ENVIRONMENT=sandbox
SQUARE_WEBHOOK_SECRET=...
# Square Plan Variation IDs (créer dans Square Dashboard > Subscriptions > Plans)
SQUARE_PLAN_VARIATION_ID_PRO=...
SQUARE_PLAN_VARIATION_ID_PREMIUM=...

# App URL
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_BASE_URL=http://localhost:3000

# Google reCAPTCHA (pour protection spam sur formulaires)
# Obtenir les clés sur: https://www.google.com/recaptcha/admin
# IMPORTANT: Ajouter 'localhost' dans les domaines autorisés pour le développement local
# Clés de test Google (fonctionnent toujours sur localhost):
# NEXT_PUBLIC_RECAPTCHA_SITE_KEY=6LeIxAcTAAAAAJcZVRqyHh71UMIEGNQ_MXjiZKhI
# RECAPTCHA_SECRET_KEY=6LeIxAcTAAAAAGG-vFI1TnRWxMZNFuojJ4WifJWe
NEXT_PUBLIC_RECAPTCHA_SITE_KEY=your-site-key-here
RECAPTCHA_SECRET_KEY=your-secret-key-here

# Upstash Redis (pour rate limiting distribué - optionnel)
# Créer un compte sur: https://upstash.com
# Si non configuré, le rate limiting utilisera un système en mémoire
UPSTASH_REDIS_REST_URL=https://your-redis-url.upstash.io
UPSTASH_REDIS_REST_TOKEN=your-token-here